<p id="notice"><%= notice %></p>

<p>
  <strong>Valor:</strong>
  <%= @multum.valor %>
</p>

<p>
  <strong>Fecha:</strong>
  <%= @multum.fecha %>
</p>

<p>
  <strong>Estado:</strong>
  <%= @multum.estado %>
</p>

<p>
  <strong>Prestamo:</strong>
  <%= @multum.Prestamo.id.to_s + " - " + @multum.Prestamo.Usuario.nombres + " " + @multum.Prestamo.Usuario.apellidos + " | " + @multum.Prestamo.fecha.to_s %>
</p>

<%= link_to 'Editar', edit_multum_path(@multum), :class => "btn btn-warning text-white" %> |
<%= link_to 'Volver', multa_path, :class => "btn btn-danger text-white" %>
